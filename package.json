{
	"name": "@calebgasser/svelte-ghblog",
	"version": "0.1.31",
	"description": "Create a simple blog from a GitHub repo using Svelte",
	"keywords": [
		"svelte",
		"blog",
		"github"
	],
	"homepage": "https://github.com/calebgasser/svelte-ghblog/tree/develop#readme",
	"bugs": {
		"url": "https://github.com/calebgasser/svelte-ghblog/issues",
		"email": "calebgasser@gmail.com"
	},
	"license": "GPL-3.0-only",
	"author": {
		"name": "Caleb Gasser",
		"email": "calebgasser@gmail.com",
		"url": "https://www.calebgasser.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/calebgasser/svelte-ghblog.git"
	},
	"lint-staged": {
		"*.js": [
			"prettier --write --plugin-search-dir=. .",
			"prettier --check --plugin-search-dir=. .",
			"eslint ."
		],
		"*.ts": [
			"prettier --write --plugin-search-dir=. .",
			"prettier --check --plugin-search-dir=. .",
			"eslint ."
		]
	},
	"main": "src/index.ts",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage",
		"prepare": "svelte-kit sync && husky install",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@commitlint/types": "^17.0.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@testing-library/svelte": "^3.1.3",
		"@types/marked": "^4.0.3",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"c8": "^7.12.0",
		"depcheck": "^1.4.3",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.0",
		"jsdom": "^20.0.0",
		"lint-staged": "^13.0.3",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-highlight": "^6.2.0",
		"svelte-markdown": "^0.2.2",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.0",
		"vitest": "^0.28.4",
		"zod": "^3.17.10"
	},
	"type": "module"
}
